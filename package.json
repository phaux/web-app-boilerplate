{
  "name": "app",
  "version": "0.0.0",
  "license": "ISC",
  "scripts": {
    "build": "rollup --config",
    "format": "prettier --write \"src/**\" \"styles/**\" && tslint -p . --fix && stylelint \"styles/**/*.css\" --fix",
    "start": "pwa-serve -d app=src -w src -w styles",
    "test": "tsc -p . && tslint -p . && stylelint \"styles/**/*.css\""
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "prettier": "^1.16.4",
    "pwa-serve": "^0.2.1",
    "rollup": "^1.7.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "stylelint": "^9.10.1",
    "stylelint-config-idiomatic-order": "^6.2.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.14.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "mobx": "^5.9.4",
    "mobx-preact": "^3.0.0",
    "mobx-state-tree": "^3.12.2",
    "mobx-utils": "^5.2.0",
    "preact": "^10.0.0-alpha.2"
  }
}
