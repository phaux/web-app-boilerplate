{
  "extends": [
    "tslint:all",
    "tslint-react",
    "tslint-eslint-rules"
  ],
  "rules": {
    "align": {"severity": "warn", "options": ["parameters", "statements", "members", "elements"]},
    "array-bracket-spacing": {"severity": "warn", "options": "never"},
    "arrow-parens": {"severity": "warn", "options": "ban-single-arg-parens"},
    "block-spacing": {"severity": "warn"},
    "comment-format": false,
    "completed-docs": false,
    "curly": false,
    "file-name-casing": [true, "kebab-case"],
    "interface-name": false,
    "interface-over-type-literal": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-space-before-trailing-slash": {"severity": "warn"},
    "max-classes-per-file": false,
    "max-line-length": [true, 120],
    "member-access": false,
    "member-ordering": {"severity": "warn"},
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-any": false,
    "no-console": false,
    "no-default-export": false,
    "no-empty": false,
    "no-inferrable-types": false,
    "no-import-side-effect": false,
    "no-magic-numbers": false,
    "no-non-null-assertion": false,
    "no-null-keyword": false,
    "no-parameter-properties": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": false,
    "no-this-assignment": [true, {"allow-destructuring": true}],
    "no-trailing-whitespace": {"severity": "warn"},
    "no-unbound-method": false,
    "no-unsafe-any": false,
    "no-unused-variable": false,
    "object-curly-spacing": {"severity": "warn"},
    "ordered-imports": {"severity": "warn"},
    "one-line": {"severity": "warn", "options": ["check-open-brace", "check-whitespace"]},
    "only-arrow-functions": false,
    "prefer-function-over-method": false,
    "prefer-template": false,
    "promise-function-async": false,
    "semicolon": {"severity": "warn", "options": "never"},
    "strict-boolean-expressions": false,
    "strict-type-predicates": false,
    "ter-indent": {"severity": "warn", "options": [2, {"SwitchCase": 1}]},
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "typedef": [true, "parameter"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "whitespace": {"severity": "warn"}
  },
  "jsRules": {
    "align": {"severity": "warn", "options": ["parameters", "statements", "members", "elements"]},
    "array-bracket-spacing": {"severity": "warn", "options": "never"},
    "arrow-parens": {"severity": "warn", "options": "ban-single-arg-parens"},
    "block-spacing": {"severity": "warn"},
    "comment-format": false,
    "completed-docs": false,
    "curly": false,
    "max-line-length": [true, 120],
    "member-ordering": {"severity": "warn"},
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-console": false,
    "no-default-export": false,
    "no-empty": false,
    "no-import-side-effect": false,
    "no-magic-numbers": false,
    "no-null-keyword": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": false,
    "no-trailing-whitespace": {"severity": "warn"},
    "no-unused-variable": false,
    "object-curly-spacing": {"severity": "warn"},
    "ordered-imports": {"severity": "warn"},
    "one-line": {"severity": "warn", "options": ["check-open-brace", "check-whitespace"]},
    "only-arrow-functions": false,
    "prefer-function-over-method": false,
    "prefer-template": false,
    "promise-function-async": false,
    "semicolon": {"severity": "warn", "options": "never"},
    "ter-indent": {"severity": "warn", "options": [2, {"SwitchCase": 1}]},
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "whitespace": {"severity": "warn"}
  }
}
