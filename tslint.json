{
  "extends": ["tslint:all", "tslint-react", "tslint-eslint-rules", "tslint-consistent-codestyle"],
  "rules": {
    "align": [true, "parameters", "statements", "members", "elements"],
    "array-bracket-spacing": [true, "never"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "block-spacing": true,
    "brace-style": [true, "stroustrup", { "allowSingleLine": true }],
    "comment-format": false,
    "completed-docs": false,
    "curly": false,
    "early-exit": true,
    "file-name-casing": [true, "kebab-case"],
    "interface-name": false,
    "interface-over-type-literal": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-space-before-trailing-slash": true,
    "max-classes-per-file": false,
    "max-line-length": [true, 100],
    "member-access": false,
    "member-ordering": [true, { "order": "instance-sandwich" }],
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-any": false,
    "no-console": false,
    "no-default-export": false,
    "no-empty": false,
    "no-inferrable-types": false,
    "no-import-side-effect": false,
    "no-magic-numbers": false,
    "no-non-null-assertion": false,
    "no-null-keyword": false,
    "no-parameter-properties": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": false,
    "no-this-assignment": [true, { "allow-destructuring": true }],
    "no-trailing-whitespace": true,
    "no-unbound-method": false,
    "no-unnecessary-callback-wrapper": false,
    "no-unnecessary-else": true,
    "no-unsafe-any": false,
    "no-unused-variable": false,
    "no-use-before-declare": false,
    "object-curly-spacing": true,
    "ordered-imports": true,
    "one-line": [true, "check-open-brace", "check-whitespace"],
    "only-arrow-functions": false,
    "prefer-function-over-method": false,
    "prefer-template": false,
    "promise-function-async": false,
    "semicolon": [true, "never"],
    "strict-boolean-expressions": false,
    "strict-type-predicates": false,
    "ter-arrow-spacing": true,
    "ter-computed-property-spacing": true,
    "ter-func-call-spacing": true,
    "ter-indent": [true, 2, { "SwitchCase": 1 }],
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "typedef": false,
    "variable-name": false,
    "whitespace": true
  },
  "jsRules": {
    "align": [true, "parameters", "statements", "members", "elements"],
    "array-bracket-spacing": [true, "never"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "block-spacing": true,
    "comment-format": false,
    "completed-docs": false,
    "curly": false,
    "early-exit": true,
    "max-line-length": [true, 120],
    "member-ordering": [true, { "order": "instance-sandwich" }],
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "no-console": false,
    "no-default-export": false,
    "no-empty": false,
    "no-import-side-effect": false,
    "no-magic-numbers": false,
    "no-null-keyword": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": false,
    "no-trailing-whitespace": true,
    "no-unnecessary-else": true,
    "no-unused-variable": false,
    "no-use-before-declare": false,
    "object-curly-spacing": true,
    "ordered-imports": true,
    "one-line": [true, "check-open-brace", "check-whitespace"],
    "only-arrow-functions": false,
    "prefer-function-over-method": false,
    "prefer-template": false,
    "promise-function-async": false,
    "semicolon": [true, "never"],
    "ter-arrow-spacing": true,
    "ter-computed-property-spacing": true,
    "ter-func-call-spacing": true,
    "ter-indent": [true, 2, { "SwitchCase": 1 }],
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "variable-name": false,
    "whitespace": true
  }
}
